{
  "version": "0.2.0",
  "configurations": [{
    "name": "mocha",
    "type": "node",
    "request": "launch",
    "program": "${workspaceFolder}/node_modules/.bin/_mocha",
    "stopOnEntry": false,
    "args": [
      "--compilers",
      "js:babel-core/register",
      "--no-timeouts",
      "--colors",
      "tests"
    ],
    "cwd": "${workspaceRoot}",
    "runtimeExecutable": null,
    "env": {
      "NODE_ENV": "testing"
    }
  }]
}
